include header
.container
  table.table
   tr
     td id
     td name
     td type
                - items.forEach(function(item){
                     tr
                       td 
                         a(href="/remove/item/#{item._id}") remove #{item._id}
                       td #{item.name} 
                       - if(!item.masterId)
                           td Serie
                       - else
                           td Item
                           td SerieId #{item.masterId} 
                         
                - })